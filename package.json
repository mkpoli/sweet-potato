{
  "name": "sweet-potato",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "dev:legacy": "MODE=legacy next dev --hostname 0.0.0.0",
    "build": "rm -rf ./build && NODE_ENV=production next build",
    "start": "next start -p 8080",
    "lint": "eslint src/** --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "lint:fix": "eslint src/** --ext .ts,.tsx --fix --no-error-on-unmatched-pattern && yarn format",
    "format": "prettier --config .prettierrc.json --write '**/*.tsx' --loglevel silent",
    "test": "echo 'pass'",
    "storybook": "rm -rf ./node_modules/.cache && start-storybook -p 6006 --no-open --quiet",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "screenshot": "rm -rf ./__screenshots__ && storycap http://localhost:5555 --serverCmd 'start-storybook -p 5555 --ci'"
  },
  "dependencies": {
    "@aspida/fetch": "^1.7.1",
    "@chakra-ui/react": "^1.8.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "axios": "^0.25.0",
    "canvas": "^2.9.0",
    "firebase": "^9.6.6",
    "firebase-admin": "^10.0.2",
    "framer-motion": "^5",
    "next": "12.0.10",
    "nextjs-progressbar": "^0.0.13",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-file-input-previews-base64": "^1.0.8",
    "react-icons": "^4.3.1",
    "recoil": "^0.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@gogaille/storybook-addon-next-router": "^4.0.0",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@types/node": "17.0.14",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "babel-loader": "^8.2.3",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "husky": "^7.0.0",
    "prettier": "^2.5.1",
    "storybook-addon-turbo-build": "^1.0.1",
    "storycap": "^3.1.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.5.5"
  }
}
